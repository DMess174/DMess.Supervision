<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:employeeview="clr-namespace:DMess.Supervision.Desktop.Views.Tabs.Employees"
    xmlns:employeevm="clr-namespace:DMess.Supervision.Desktop.ViewModels.Tabs.Employees"
    xmlns:producttypeview="clr-namespace:DMess.Supervision.Desktop.Views.Tabs"
    xmlns:producttypevm="clr-namespace:DMess.Supervision.Desktop.ViewModels.Tabs.Products"
    xmlns:views="clr-namespace:DMess.Supervision.Desktop.Views"
    xmlns:vm="clr-namespace:DMess.Supervision.Desktop.ViewModels">

    <DataTemplate DataType="{x:Type vm:HeadViewModel}">
        <views:HeadView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:MainViewModel}">
        <views:MainView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:LoginViewModel}">
        <views:LoginView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AccountViewModel}">
        <views:AccountView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type producttypevm:ProductTypesViewModel}">
        <producttypeview:ProductTypesView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type employeevm:EmployeeCollectionViewModel}">
        <employeeview:EmployeeCollectionView />
    </DataTemplate>
</ResourceDictionary>